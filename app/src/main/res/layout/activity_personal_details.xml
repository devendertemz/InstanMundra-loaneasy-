<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    android:weightSum="7">

    <ImageView
        android:id="@+id/ivbg"
        android:layout_width="match_parent"
        android:layout_height="320dp"
        android:layout_alignParentTop="true"
        android:scaleType="fitStart"
        app:srcCompat="@drawable/phone_bg" />

    <customfonts.TextView_Roboto_regular
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dimen_30dp"
        android:layout_marginTop="@dimen/dimen_20dp"
        android:layout_marginRight="15dp"
        android:layout_weight="1"
        android:text="Personal details"
        android:textColor="@color/white"
        android:textSize="@dimen/dimen_20sp" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/btSubmitUserInfo"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="@dimen/dimen_65dp"
        android:layout_marginRight="15dp"
        android:elevation="@dimen/dimen_4dp"
        android:scrollbars="none">

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="@dimen/dimen_5dp"
            app:cardCornerRadius="@dimen/dimen_10dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <customfonts.TextView_Roboto_light
                    android:id="@+id/tvNameWlcm"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/dimen_15dp"
                    android:fontFamily="serif"
                    android:gravity="center"
                    android:text="Please enter your personal details !"
                    android:textColor="@color/gray_700"
                    android:textSize="@dimen/dimen_16sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:layout_marginRight="@dimen/dimen_5dp"
                    android:layout_marginLeft="@dimen/dimen_5dp"
                    android:orientation="horizontal"
                    android:weightSum="2">

                    <customfonts.TextView_Roboto_regular
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="5dp"
                        android:layout_weight="1"
                        android:text="First Name"
                        android:textColor="@color/carmine"
                        android:textSize="@dimen/dimen_15sp" />


                    <customfonts.TextView_Roboto_regular
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="12dp"
                        android:layout_weight="1"
                        android:text="Last Name"
                        android:textColor="@color/carmine"
                        android:textSize="@dimen/dimen_15sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/dimen_5dp"
                    android:layout_marginLeft="@dimen/dimen_5dp"
                    android:orientation="horizontal"
                    android:weightSum="2">

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/input_layout_Fname"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginRight="5dp"
                        android:layout_weight="1"
                        android:theme="@style/TextLabel">

                        <EditText
                            android:id="@+id/etFirstName"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:drawablePadding="@dimen/dimen_5dp"
                            android:focusable="true"
                            android:focusableInTouchMode="true"
                            android:inputType="textCapCharacters"
                            android:maxLines="1"
                            android:paddingLeft="10dp"
                            android:paddingRight="10dp"
                            android:paddingBottom="10dp"
                            android:paddingTop="@dimen/dimen_5dp"
                            android:textColor="@color/black"
                            android:textColorHint="@color/gray_200"
                            android:theme="@style/ThemeEditText" />

                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/input_layout_Lname"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginRight="5dp"
                        android:layout_weight="1"
                        android:theme="@style/TextLabel">

                        <EditText
                            android:id="@+id/etLastName"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:drawablePadding="@dimen/dimen_5dp"
                            android:focusableInTouchMode="true"
                            android:inputType="textCapCharacters"
                            android:maxLines="1"
                            android:paddingLeft="10dp"
                            android:paddingRight="10dp"
                            android:paddingBottom="10dp"
                            android:paddingTop="@dimen/dimen_5dp"
                            android:textColor="@color/black"
                            android:textColorHint="@color/black"
                            android:theme="@style/ThemeEditText" />



                    </com.google.android.material.textfield.TextInputLayout>

                </LinearLayout>

                <customfonts.TextView_Roboto_regular
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginRight="@dimen/dimen_5dp"
                    android:layout_marginLeft="@dimen/dimen_10dp"
                    android:layout_marginTop="@dimen/dimen_10dp"
                    android:text="Enter Email"
                    android:textColor="@color/carmine"
                    android:textSize="@dimen/dimen_15sp" />


                <EditText
                    android:id="@+id/etOfficialEmail"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/dimen_5dp"
                    android:layout_marginLeft="@dimen/dimen_5dp"
                    android:focusableInTouchMode="true"
                    android:inputType="textEmailAddress"
                    android:maxLines="1"
                    android:paddingLeft="5dp"
                    android:paddingBottom="10dp"
                    android:textColor="@color/black"
                    android:textSize="@dimen/dimen_18sp"
                    android:theme="@style/ThemeEditText" />



                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_20dp"
                    android:layout_marginLeft="@dimen/dimen_10dp"
                    android:layout_marginRight="@dimen/dimen_10dp"
                    android:orientation="horizontal"
                    android:weightSum="2">

                    <customfonts.TextView_Roboto_regular
                        android:id="@+id/tvGenderText"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="5dp"
                        android:layout_weight="1"
                        android:text="Gender"
                        android:textColor="@color/carmine"
                        android:textSize="@dimen/dimen_15sp" />


                    <customfonts.TextView_Roboto_regular
                        android:id="@+id/tvDobText"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/dimen_25dp"
                        android:layout_weight="1"
                        android:text="Date of birth"
                        android:textColor="@color/carmine"
                        android:textSize="@dimen/dimen_15sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dimen_10dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginRight="@dimen/dimen_10dp"
                    android:orientation="horizontal"
                    android:weightSum="2">

                    <FrameLayout
                        android:layout_width="@dimen/dimen_60dp"
                        android:layout_height="30dp"
                        android:layout_marginRight="@dimen/dimen_10dp"
                        android:layout_weight="1"
                        android:background="@drawable/rectangle" >

                        <androidx.appcompat.widget.AppCompatSpinner
                            android:id="@+id/spnGender"
                            style="@style/spinner_style"
                            android:theme="@style/ThemeSpinner"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dimen_5dp"
                            android:layout_marginRight="@dimen/dimen_10dp"
                            android:dropDownVerticalOffset="50dp"
                            android:spinnerMode="dropdown"
                            app:backgroundTint="@color/carmine" />
                    </FrameLayout>

                    <RelativeLayout
                        android:layout_width="@dimen/dimen_60dp"
                        android:layout_height="@dimen/dimen_30dp"
                        android:layout_gravity="start"
                        android:layout_marginLeft="15dp"
                        android:layout_marginTop="@dimen/dimen_13dp"
                        android:layout_weight="1"
                        android:background="@drawable/rectangle"
                        android:paddingRight="-40dp">
                        <!--  <LinearLayout
                              android:layout_width="0dp"
                              android:layout_height="wrap_content"

                              android:layout_weight="1"
                              android:orientation="vertical">-->

                        <TextView
                            android:id="@+id/tvDob"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/dimen_50dp"
                            android:drawableRight="@drawable/calendar_svg"
                            android:drawablePadding="@dimen/dimen_30dp"
                            android:hint="Select"
                            android:padding="6dp"
                            android:textColor="@color/gray_800"
                            android:textColorHint="@color/gray_700"
                            android:textSize="@dimen/dimen_14sp" />

                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_10dp"
                    android:layout_marginLeft="@dimen/dimen_10dp"
                    android:layout_marginRight="@dimen/dimen_10dp"
                    android:orientation="horizontal"
                    android:weightSum="2">

                    <customfonts.TextView_Roboto_regular
                        android:id="@+id/tvCurrentLoanText"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:text="Current Loan"
                        android:textColor="@color/carmine"
                        android:textSize="@dimen/dimen_15sp" />


                    <customfonts.TextView_Roboto_regular
                        android:id="@+id/tvHouseTypeText"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="27dp"
                        android:layout_weight="1"
                        android:text="House Type"
                        android:textColor="@color/carmine"
                        android:textSize="@dimen/dimen_15sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="@dimen/dimen_10dp"
                    android:layout_marginRight="@dimen/dimen_10dp"
                    android:orientation="horizontal"
                    android:paddingBottom="@dimen/dimen_5dp"
                    android:weightSum="2">

                    <FrameLayout
                        android:layout_width="@dimen/dimen_60dp"
                        android:layout_height="@dimen/dimen_30dp"
                        android:layout_marginRight="@dimen/dimen_10dp"
                        android:layout_weight="1"
                        android:background="@drawable/rectangle">

                        <androidx.appcompat.widget.AppCompatSpinner
                            android:id="@+id/spnCurrentLoan"
                            style="@style/spinner_style"
                            android:theme="@style/ThemeSpinner"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:dropDownVerticalOffset="50dp"
                            android:spinnerMode="dropdown"
                            app:backgroundTint="@color/carmine" />
                    </FrameLayout>

                    <FrameLayout
                        android:layout_width="@dimen/dimen_60dp"
                        android:layout_height="@dimen/dimen_30dp"
                        android:layout_marginLeft="@dimen/dimen_10dp"
                        android:layout_weight="1"
                        android:background="@drawable/rectangle">

                        <androidx.appcompat.widget.AppCompatSpinner
                            android:id="@+id/spnHouseType"
                            android:theme="@style/ThemeSpinner"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="10dp"
                            android:layout_weight="1"
                            style="@style/spinner_style"
                            android:dropDownVerticalOffset="50dp"
                            android:spinnerMode="dropdown"
                            app:backgroundTint="@color/carmine" />
                    </FrameLayout>
                </LinearLayout>


                <EditText
                    android:id="@+id/etPinCode"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="@dimen/dimen_20dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="10dp"
                    android:textCursorDrawable="@drawable/color_cursor"
                    android:background="@drawable/shape_rect_bckground_gray"
                    android:inputType="number"
                    android:gravity="left"
                    android:maxLines="1"
                    android:hint="PINCODE"
                    android:maxLength="6"
                    android:paddingTop="@dimen/dimen_10dp"
                    android:paddingBottom="@dimen/dimen_8dp"
                    android:paddingLeft="@dimen/dimen_10dp"
                    android:paddingRight="@dimen/dimen_75dp"
                    android:textColor="@color/black"
                    android:theme="@style/ThemeEditText" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_10dp"
                    android:layout_marginLeft="@dimen/dimen_10dp"
                    android:layout_marginRight="@dimen/dimen_10dp"
                    android:orientation="horizontal"
                    android:weightSum="2">

                    <customfonts.TextView_Roboto_regular
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="5dp"
                        android:layout_weight="1"
                        android:text="State"
                        android:textColor="@color/carmine"
                        android:textSize="@dimen/dimen_15sp" />
                    
                    <customfonts.TextView_Roboto_regular
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/dimen_25dp"
                        android:layout_weight="1"
                        android:text="City"
                        android:textColor="@color/carmine"
                        android:textSize="@dimen/dimen_15sp" />

                </LinearLayout>




                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="@dimen/dimen_10dp"
                    android:layout_marginRight="@dimen/dimen_10dp"
                    android:orientation="horizontal"
                    android:paddingBottom="@dimen/dimen_5dp"
                    android:weightSum="2">

                    <FrameLayout
                        android:layout_width="@dimen/dimen_60dp"
                        android:layout_height="@dimen/dimen_40dp"
                        android:layout_marginRight="@dimen/dimen_10dp"
                        android:layout_weight="1"
                        android:background="@drawable/rectangle">

                        <EditText
                            android:id="@+id/etState"
                            android:theme="@style/ThemeEditText2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textCursorDrawable="@drawable/color_cursor"/>
                    </FrameLayout>

                    <FrameLayout
                        android:layout_width="@dimen/dimen_60dp"
                        android:layout_height="@dimen/dimen_40dp"
                        android:layout_marginLeft="@dimen/dimen_10dp"
                        android:layout_weight="1"
                        android:background="@drawable/rectangle">

                        <EditText
                            android:id="@+id/etCity"
                            android:theme="@style/ThemeEditText2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textCursorDrawable="@drawable/color_cursor"/>
                    </FrameLayout>
                </LinearLayout>

                <customfonts.TextView_Roboto_regular
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dimen_10dp"
                    android:layout_marginTop="@dimen/dimen_20dp"
                    android:layout_weight="1"
                    android:text="Local Address"
                    android:textColor="@color/carmine"
                    android:textSize="@dimen/dimen_15sp" />

                <EditText
                    android:id="@+id/etLocalAddress"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="@dimen/dimen_10dp"
                    android:layout_marginRight="@dimen/dimen_10dp"
                    android:layout_marginBottom="@dimen/dimen_10dp"
                    android:textCursorDrawable="@drawable/color_cursor"
                    android:background="@drawable/shape_rect_bckground_gray"
                    android:inputType="textCapCharacters"
                    android:maxLines="10"
                    android:lines="10"
                    android:paddingLeft="@dimen/dimen_5dp"
                    android:paddingRight="@dimen/dimen_5dp"
                    android:paddingTop="@dimen/dimen_5dp"
                    android:paddingBottom="@dimen/dimen_60dp"
                    android:textColor="@color/black"
                    android:theme="@style/ThemeEditText" />



                <LinearLayout
                    android:id="@+id/lyLoanEmi"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dimen_5dp"
                    android:layout_marginTop="@dimen/dimen_10dp"
                    android:layout_marginRight="@dimen/dimen_5dp"
                    android:orientation="vertical"
                    android:visibility="gone">

                    <customfonts.TextView_Roboto_regularcondensed
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/dimen_5dp"
                        android:layout_weight="1"
                        android:text="Total Current Loan EMI"
                        android:textSize="17sp" />

                    <customfonts.EditText_Roboto_Light
                        android:id="@+id/etLoanEmi"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:drawableLeft="@drawable/rupee_svg"
                        android:drawablePadding="@dimen/dimen_10dp"
                        android:focusable="true"
                        android:focusableInTouchMode="true"
                        android:hint="EMI Amount"
                        android:inputType="number"
                        android:maxLines="1"
                        android:padding="10dp"
                        android:theme="@style/TextLabel" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="@dimen/dimen_10dp"
                        android:layout_marginRight="@dimen/dimen_10dp"
                        android:background="@color/colorPrimary" />

                </LinearLayout>



                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_15dp"
                    android:orientation="vertical"
                    android:weightSum="2">

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/input_layout_ReferenceName1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginRight="10dp"
                        android:theme="@style/TextLabel">

                        <customfonts.EditText_Roboto_Light
                            android:id="@+id/etReferenceName1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:drawablePadding="@dimen/dimen_5dp"
                            android:focusable="true"
                            android:focusableInTouchMode="true"
                            android:inputType="textCapCharacters"
                            android:maxLength="100"
                            android:textSize="@dimen/dimen_17sp"
                            android:hint="Please enter your Primary Reference Name"
                            android:maxLines="1"
                            android:paddingLeft="5dp"
                            android:paddingTop="10dp"
                            android:paddingRight="10dp"
                            android:paddingBottom="12dp"
                            android:textColor="@color/black"
                            android:textColorHighlight="@color/white"
                            android:theme="@style/ThemeEditText" />
                    </com.google.android.material.textfield.TextInputLayout>


                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/input_layout_ReferencePhone1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginRight="10dp"
                        android:theme="@style/TextLabel">

                        <customfonts.EditText_Roboto_Light
                            android:id="@+id/etReferencePhone1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:drawablePadding="@dimen/dimen_5dp"
                            android:focusable="true"
                            android:focusableInTouchMode="true"
                            android:inputType="phone"
                            android:maxLength="10"
                            android:textSize="@dimen/dimen_17sp"
                            android:hint="Primary Reference Phone No"
                            android:maxLines="1"
                            android:paddingLeft="5dp"
                            android:paddingTop="10dp"
                            android:paddingRight="10dp"
                            android:paddingBottom="12dp"
                            android:textColor="@color/black"
                            android:textColorHighlight="@color/white"
                            android:theme="@style/ThemeEditText" />
                    </com.google.android.material.textfield.TextInputLayout>



                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_15dp"
                    android:orientation="vertical"
                    android:weightSum="2">

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/input_layout_ReferenceName2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginRight="10dp"
                        android:theme="@style/TextLabel">

                        <customfonts.EditText_Roboto_Light
                            android:id="@+id/etReferenceName2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:drawablePadding="@dimen/dimen_5dp"
                            android:focusable="true"
                            android:focusableInTouchMode="true"
                            android:inputType="textCapCharacters"
                            android:maxLength="100"
                            android:textSize="@dimen/dimen_17sp"
                            android:hint="Please enter your secondary reference Name"
                            android:maxLines="1"
                            android:paddingLeft="5dp"
                            android:paddingTop="10dp"
                            android:paddingRight="10dp"
                            android:paddingBottom="12dp"
                            android:textColor="@color/black"
                            android:textColorHighlight="@color/white"
                            android:theme="@style/ThemeEditText" />
                    </com.google.android.material.textfield.TextInputLayout>


                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/input_layout_ReferencePhone2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginRight="10dp"
                        android:theme="@style/TextLabel">

                        <customfonts.EditText_Roboto_Light
                            android:id="@+id/etReferencePhone2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:drawablePadding="@dimen/dimen_5dp"
                            android:focusable="true"
                            android:focusableInTouchMode="true"
                            android:inputType="phone"
                            android:maxLength="10"
                            android:textSize="@dimen/dimen_17sp"
                            android:hint="Secondary reference Phone No"
                            android:maxLines="1"
                            android:paddingLeft="5dp"
                            android:paddingTop="10dp"
                            android:paddingRight="10dp"
                            android:paddingBottom="12dp"
                            android:textColor="@color/black"
                            android:textColorHighlight="@color/white"
                            android:theme="@style/ThemeEditText" />
                    </com.google.android.material.textfield.TextInputLayout>



                </LinearLayout>


            </LinearLayout>

        </androidx.cardview.widget.CardView>
    </ScrollView>

    <customfonts.MyTextView_Roboto_Medium
        android:id="@+id/btSubmitUserInfo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerInParent="true"
        android:background="@drawable/corner_button_background"
        android:clickable="true"
        android:foreground="?attr/selectableItemBackground"
        android:gravity="center"
        android:text="Submit"
        android:textColor="@color/white"
        android:textSize="@dimen/dimen_20sp" />

</RelativeLayout>


